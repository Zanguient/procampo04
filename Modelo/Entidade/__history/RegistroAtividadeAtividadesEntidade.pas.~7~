        FDataAtividade: TDate;
        FProduto: TProdutoEntidade;
        FQuantidade: double;
        FUnidade: AnsiString;
        FObservacao: AnsiString;
      public
        FCodigo: Integer;
        FCodigoRegistroAtividade: integer;
        FAtividade: TAtividadeEntidade;
        FDataAtividade: TDate;
        FProduto: TProdutoEntidade;
        FQuantidade: double;
        FUnidade: AnsiString;
        FObservacao: AnsiString;
    end;
implementation

end.
